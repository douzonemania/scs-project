
<select class="custom-select my-1 mr-sm-3" style="width:73%" id="mobileSecondOption">
	<option value="X" selected="selected">-----------</option>	
	
	<%
	for(var i=0; i<data.length; i++){
		var vo = data[i];	
	%>
	
	<% if(vo.stock==0) { %>
		<option data-no = "0" value="0">
			<%=vo.name%>품절
		</option>
	<% } %>
		<option data-no = "<%=vo.stock%>" value="<%=vo.no%>"><%=vo.name%></option>
	<%
	}
	%>	
</select>

